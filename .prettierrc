{
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "avoid",
    "endOfLine": "lf",
    "singleAttributePerLine": true,
    "importOrder": [
      "^global/(.*)$",
      "^@propellerads/landings-services(.*)$",
      "^@propellerads/landings-storage-service(.*)$",
      "^services/(.*)$",
      "^landings/(.*)$",
      "^context/(.*)$",
      "^@propellerads/landings-hooks(.*)$",
      "^hooks/(.*)$",
      "^@propellerads/landings-features(.*)$",
      "^features/(.*)$",
      "^@propellerads/landings-ui(.*)$",
      "^components/(.*)$",
      "^utils/(.*)$",
      "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "plugins": ["@trivago/prettier-plugin-sort-imports"]    
}
  